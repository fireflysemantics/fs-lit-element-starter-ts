{
  "name": "fs-typescript-starter",
  "version": "1.1.0",
  "description": "A typescript starter project by Firefly Semantics Corporation",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "npx release-it",
    "prepare": "husky"
  },
  "bugs": "https://github.com/fireflysemantics/fs-typscript-starter/issues",
  "homepage": "https://github.com/fireflysemantics/fs-typscript-starter/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fireflysemantics/fs-typscript-starter.git"
  },
  "keywords": [
    "starter-project",
    "github",
    "flow",
    "pull-requests",
    "README",
    "LICENSE",
    "Typescript",
    "typescript-eslint",
    "prettier",
    "lint-staged",
    "husky",
    "hooks",
    "git",
    "testing"
  ],
  "author": "Firefly Semantics Corporation",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@release-it/conventional-changelog": "^8.0.1",
    "husky": "^9.0.11",
    "release-it": "^17.4.0"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}"
    },
    "github": {
      "release": false
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "infile": "CHANGELOG.md",
        "preset": {
          "name": "conventionalcommits",
          "type": [
            {
              "type": "feat",
              "section": "Features"
            },
            {
              "type": "fix",
              "section": "Bug Fixes"
            }
          ]
        }
      }
    }
  }
}
